{
  "version": 2,
  "name": "watch-together-frontend",
  "env": {
    "REACT_APP_SERVER_URL": "https://watch-together-519k.onrender.com"
  },
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "npm run build:production"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ]
}
